{
  "name": "tourze/youzan-api-bundle",
  "type": "symfony-bundle",
  "description": "有赞API集成Bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "youzanyun/open-sdk": "^2.0",
    "tourze/easy-admin-attribute": "0.1.*",
    "symfony/dependency-injection": "^6.4",
    "symfony/config": "^6.4",
    "doctrine/orm": "^3.0",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/http-kernel": "^6.4",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "doctrine/persistence": "^3.1 || ^4"
  },
  "autoload": {
    "psr-4": {
      "YouzanApiBundle\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload-dev": {
    "psr-4": {
      "YouzanApiBundle\\Tests\\": "tests/"
    }
  }
}